{
  "name": "bayuk",
  "private": true,
  "version": "1.0.0",
  "description": "A buy and sell application",
  "main": "server.js",
  "scripts": {
    "postinstall": "npm run build",
    "test:app": "karma run",
    "test:api": "better-npm-run test:api",
    "start": "better-npm-run start",
    "build:server": "babel server/main-es6.js -o server/main.js",
    "build:client": "webpack -p --config webpack.prod.config.js --progress",
    "build": "better-npm-run build",
    "karma": "karma start",
    "clean": "rimraf dist/* && rimraf server.js"
  },
  "betterScripts": {
    "start": {
      "command": "babel-watch server/main-es6.js"
    },
    "build": {
      "command": "npm run clean && npm run build:server && npm run build:client",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "test:api": {
      "command": "mocha \"test/api/**.js\" --compilers js:babel-core/register",
      "env": {
        "NODE_ENV": "test"
      }
    }
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lexnapoles/Bayuk.git"
  },
  "author": "Alejandro Napoles",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lexnapoles/Bayuk/issues"
  },
  "homepage": "https://github.com/lexnapoles/Bayuk#readme",
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.2.2",
    "babel-preset-react": "^6.23.0",
    "babel-watch": "^2.0.6",
    "better-npm-run": "^0.0.15",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-http": "^3.0.0",
    "css-loader": "^0.27.3",
    "enzyme": "^2.3.0",
    "eslint": "^3.18.0",
    "eslint-loader": "^1.7.0",
    "eslint-plugin-react": "^6.10.3",
    "faker": "^4.1.0",
    "file-loader": "^0.10.1",
    "karma": "^1.5.0",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^1.3.0",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "mocha": "^3.2.0",
    "node-uuid": "^1.4.7",
    "normalize.css": "^6.0.0",
    "postcss-loader": "^1.3.3",
    "react-addons-test-utils": "^15.0.2",
    "react-css-modules": "^4.1.0",
    "rimraf": "^2.6.1",
