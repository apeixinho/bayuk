{
  "name": "bayuk",
  "private": true,
  "version": "1.0.0",
  "description": "A buy and sell application",
  "main": "server.js",
  "scripts": {
    "postinstall": "npm run build",
    "test": "karma run",
    "dev": "better-npm-run dev",
    "start": "node server.js",
    "build:server": "babel server-es6.js -o server.js",
    "build:client": "webpack -p --config webpack.prod.config.js --progress",
    "build": "better-npm-run build",
    "karma": "karma start",
    "clean": "rimraf dist/* && rimraf server.js"
  },
  "betterScripts": {
    "dev": {
      "command": "babel-node server-es6.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build": {
      "command": "npm run clean && npm run build:server && npm run build:client",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lexnapoles/Bayuk.git"
  },
  "author": "Alejandro Napoles",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lexnapoles/Bayuk/issues"
  },
  "homepage": "https://github.com/lexnapoles/Bayuk#readme",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "better-npm-run": "^0.0.8",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.3.0",
    "eslint": "^2.2.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^4.1.0",
    "file-loader": "^0.8.5",
    "karma": "^0.13.21",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^0.2.2",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.4.5",
    "normalize.css": "^4.1.1",
    "postcss-loader": "^0.8.2",
    "react-addons-test-utils": "^15.0.2",
    "react-css-modules": "^3.7.6",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.4",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "dependencies": {
    "express": "^4.13.4",
    "font-awesome": "^4.6.1",
    "history": "^2.0.0",
    "karma-chai": "^0.1.0",
    "nuka-carousel": "^2.0.2",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-fa": "^4.0.1",
    "react-router": "^2.4.1"
  },
  "engines": {
    "node": "5.8.0"
  }
}
