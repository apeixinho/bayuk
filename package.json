    "webpack-hot-middleware": "^2.17.1",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "express": "^4.15.2",
    "express-jwt": "^5.1.0",
    "font-awesome": "^4.6.1",
    "fs-promise": "^1.0.0",
    "history": "^2.0.0",
    "jsonwebtoken": "^7.2.1",
    "karma-chai": "^0.1.0",
    "lodash": "^4.17.4",
